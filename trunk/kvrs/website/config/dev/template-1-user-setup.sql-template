create user if not exists 'ki-kvrswebsite-d'@'localhost' identified by '~ki-kvrswebsite-d-password~';
grant  select
 , insert
 , update
 , delete
 , create
 , create routine
 , alter routine
  on kvrswebsitedb_d.*
  to 'ki-kvrswebsite-d'@'localhost'
;
--
-- Must grant PROCESS priv to primary user to include SHOW ENGINE INNODB
-- STATUS output in MySQL-related exception reports.
--
;
grant process
  on *.*
  to 'ki-kvrswebsite-d'@'localhost'
